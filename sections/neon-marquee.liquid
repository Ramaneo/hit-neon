<style>
  .font-container {
    position: relative;
    transform: rotate(4deg);
    margin-top: 100px;
  }
  .marquee {
    --gap: 1rem;
    position: relative;
    display: flex;
    overflow: hidden;
    user-select: none;
    gap: var(--gap);
    height: 100px;
  }
  .text-container {
    background: #04062a;
    line-height: normal;
    margin-bottom: 17px;
    position: relative;
    height: 100px;
  }
  .enable-animation .marquee__content {
    animation: scroll 23s linear infinite;
    scroll-behavior: smooth;
    justify-content: space-around;
  }
  .text-container ul {
    list-style-type: none;
    display: flex;
    align-items: center;
    margin: 0px;
  }
  .marquee__content {
    flex-shrink: 0;
    display: flex;
    /* justify-content: space-around; */
    gap: var(--gap);
    min-width: 100%;
    list-style: inside;
    margin: 0;
  }
  .marquee__content > * {
    flex: 0 0 auto;
    color: white;
    margin: 2px;
    border-radius: 0.25rem;
    text-align: center;
    font-size: 36px;
    font-weight: bold;
  }
  .font-container::before {
    content: '';
    background: #04062a;
    position: absolute;
    left: -8px;
    top: 0;
    height: 100%;
    width: 10px;
    transform: rotate(2deg);
  }
  .svg-container {
    position: relative;
    transform: rotate(-1deg);
  }
  .image-container {
    line-height: normal;
    background: #11020f;
    margin-top: 0px;
  }
  .marquee--reverse .marquee__content {
    animation-direction: reverse !important;
    align-items: center;
    scroll-behavior: smooth;
    padding-left: 0px !important;
    justify-content: space-around;
  }
  .svg-container:before {
    content: '';
    background: #11020f;
    position: absolute;
    left: -4px;
    top: 0px;
    height: 100%;
    width: 10px;
  }
  @keyframes scroll {
    0% {
      transform: translateX(0);
    }

    100% {
      transform: translateX(calc(-100% - var(--gap)));
    }
  }
</style>
